lue="35"/><column index="2" value="39"/><column index="3" value="73"/><column index="4" value="56"/><column index="5" value="55"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="Supplier" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="35"/><column index="2" value="70"/><column index="3" value="87"/><column index="4" value="295"/><column index="5" value="223"/><column index="6" value="135"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 4">SELECT *
FROM Material;

SELECT COUNT(*) AS number_of_different_materials
FROM Material;</sql><sql name="SQL 5">SELECT name, description, stock
FROM Material
ORDER BY stock DESC;</sql><sql name="SQL 6">SELECT id, strftime('%d-%m-%Y', PO.po_date) AS purchase_order_date, PO.po_status --strftime means string format of time -- 
FROM PO
WHERE PO.po_status = &quot;Delivered&quot;
ORDER BY PO.po_date DESC;
</sql><sql name="SQL 7">--Incorrect query, as I showed total quantity of items, not total number of orders --
SELECT s.first_name, s.last_name, s.phone, SUM(POItem.quantity) AS item_quantity
FROM Supplier s
LEFT JOIN POItem
ON s.id = POItem.po_id
GROUP BY s.first_name, s.last_name;

-- Correct query --
SELECT Supplier.first_name, Supplier.last_name, Supplier.phone, COUNT(PO.id) AS total_number_of_orders
FROM PO
JOIN Supplier On PO.supplier_id = Supplier.id
GROUP BY supplier_id
ORDER BY Supplier.last_name, Supplier.first_name;
</sql><sql name="SQL 8">SELECT name, price
FROM Material
WHERE price &gt; 100
ORDER BY price DESC;</sql><sql name="SQL 9">CREATE VIEW TopFiveMaterialsByQuantityOrdered AS
SELECT m.name, m.description, SUM(POItem.quantity) AS total_quantity_ordered
FROM Material m
LEFT JOIN POItem
ON m.id = POItem.material_id
GROUP BY m.name
ORDER BY total_quantity_ordered DESC
LIMIT 5;</sql><sql name="SQL 10">SELECT m.id, m.name, m.description, m.stock, m.price
FROM Material m
LEFT JOIN POItem
ON m.id = POItem.material_id
WHERE quantity IS NULL;

--Tom Taylor's answer--
SELECT m.id, m.name, m.description, m.stock, m.price
FROM Material m
LEFT JOIN POItem
ON m.id = POItem.material_id
WHERE POITEM.id IS NULL; --He suggests using the primary key (in this case, post order (PO) id) to determine whether an order has been made--</sql><sql name="SQL 11*">DELETE FROM Supplier
WHERE Supplier.id NOT IN (
SELECT supplier_id
FROM PO);
-- In future, designate a small set of rows you want to delete and use the &quot;IN&quot;, not &quot;NOT IN&quot;, because for larger databases, &quot;NOT IN&quot; will iterate through the whole database and may be slower.--

SELECT *
FROM Supplier;

--Alternative solution--
DELETE FROM Supplier
WHERE Supplier.id NOT IN (
SELECT DISCTINCT supplier_id
FROM PO)
</sql><sql name="SQL 12">UPDATE Material AS m
SET price = 19.99
WHERE m.id = 7;</sql><sql name="SQL 13">SELECT
    strftime('%Y-%m', PO.po_date) AS year_month, --Note: The strftime() function is used to format a time, a date, or a datetime value based on a specified format.--
    SUM(POItem.quantity) AS total_quantity
FROM PO
LEFT JOIN POItem ON PO.id = POItem.po_id
GROUP BY strftime('%Y-%m', PO.po_date)
ORDER BY year_month;
</sql><current_tab id="9"/></tab_sql></sqlb_project>
