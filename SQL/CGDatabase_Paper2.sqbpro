ormats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="Request 1">SELECT Last_Name, First_Name, Employee_No
FROM Employees
WHERE Last_Name LIKE &quot;M%&quot;;</sql><sql name="Request 2">SELECT Department_No, Last_Name, First_Name, Phone_Number
FROM employees
WHERE Job_ID = &quot;SA_REP&quot; AND Hire_Date &gt;= &quot;1998-05-24&quot;
ORDER BY Last_Name ASC;</sql><sql name="Request 3">SELECT *
FROM Jobs
WHERE Min_Salary &lt;= 4500
ORDER BY Min_Salary DESC;</sql><sql name="Request 4">SELECT DISTINCT  e.Job_ID, e.Department_No, j.Job_Title, d.Department_Name
FROM Employees e
INNER JOIN Jobs j
    ON e.Job_ID = j.Job_ID
INNER JOIN Departments d
    ON e.Department_No = d.Department_No
WHERE e.Department_No IN (10, 30);</sql><sql name="Request 5">SELECT d.Department_Name, l.Location_ID, e.Last_Name, e.Annual_Salary
FROM Employees e
INNER JOIN Departments d
ON e.Department_No = d.Department_No
INNER JOIN Locations l
ON d.Location_ID = l.Location_ID
WHERE d.Location_ID = 1700
ORDER BY d.Department_Name ASC;</sql><sql name="Request 6">SELECT Last_Name, First_Name
FROM Employees
WHERE Hire_Date Like &quot;%-06-%&quot; OR Hire_Date LIKE &quot;%-08-%&quot;
ORDER BY Last_Name ASC;</sql><sql name="Request 7">SELECT ROUND(AVG(Annual_Salary), 2) AS Average_Salary
FROM Employees;</sql><sql name="Request 8">SELECT ROUND(SUM(Annual_Salary) / 12.0, 2) AS Total_Monthly_Salary
FROM Employees
WHERE Department_No IN (&quot;80&quot;, &quot;60&quot;)</sql><sql name="Request 9">SELECT e.Department_No, d.Department_Name, COUNT(*) AS Number_Of_Employees
FROM Employees e
INNER JOIN Departments d
ON e.Department_No = d.Department_No
GROUP BY e.Department_No, d.Department_Name
HAVING Number_Of_Employees &lt; 4;</sql><sql name="Request 10">SELECT e.Department_No, d.Department_Name, COUNT(*) AS Employee_Count
FROM Employees e
INNER JOIN Departments d
ON e.Department_No = d.Department_No
GROUP BY e.Department_No, d.Department_Name
ORDER BY Employee_Count ASC
LIMIT 1;</sql><sql name="Request 11*">SELECT *
FROM Employees;

