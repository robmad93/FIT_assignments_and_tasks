('Reusable Silicone Food Bags', 'Eco-friendly and durable food storage bags made from silicone', 14.99, 150);</sql><sql name="SQL 6">SELECT COUNT(*) AS &quot;number_of_registered_customers&quot;
FROM Customer;</sql><sql name="SQL 7">SELECT name, price, stock
FROM Product
WHERE stock &gt; 100
ORDER BY stock DESC;</sql><sql name="SQL 8">SELECT id, order_date, order_status
FROM &quot;Order&quot;
WHERE order_status = &quot;Shipped&quot;
ORDER BY order_date;</sql><sql name="SQL 9">SELECT c.first_name, c.last_name, c.phone, c.email, COUNT(DISTINCT o.id) AS total_orders
FROM Customer AS c
LEFT JOIN &quot;Order&quot; AS o ON o.customer_id = c.id
GROUP BY c.id, c.first_name, c.last_name, c.phone, c.email
ORDER BY total_orders DESC;</sql><sql name="SQL 10">SELECT name, price
FROM Product
WHERE price &gt; 120.00
ORDER BY name;</sql><sql name="SQL 11">CREATE VIEW [TopThreeBestSellers] AS
SELECT p.name, p.description, p.price, COUNT(oi.order_id) AS number_of_times_ordered
FROM Product As p
INNER JOIN OrderItem As oi
ON oi.product_id = p.id
GROUP BY p.name
ORDER BY number_of_times_ordered DESC
LIMIT 3;</sql><sql name="SQL 12">SELECT p.id, p.name, p.description, p.price, p.stock
FROM Product As p
LEFT JOIN OrderItem
ON p.id = OrderItem.product_id
WHERE OrderItem.id IS NULL;</sql><sql name="SQL 13">DELETE FROM Customer
WHERE id NOT IN (
SELECT customer_id
FROM &quot;Order&quot;
WHERE customer_id IS NOT NULL
);</sql><sql name="SQL 14">UPDATE Product
SET price = 79.99
WHERE name = 'Bluetooth Headphones';</sql><sql name="SQL 15">SELECT strftime('%Y-%m', o.order_date) AS order_month, SUM(oi.quantity) AS units_sold
FROM &quot;Order&quot; AS o
JOIN OrderItem AS oi
ON o.id = oi.order_id
GROUP BY strftime('%Y-%m', o.order_date)
ORDER BY order_month ASC;</sql><current_tab id="13"/></tab_sql></sqlb_project>
